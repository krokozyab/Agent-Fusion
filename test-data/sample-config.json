{
  "name": "orchestrator",
  "version": "0.1.0",
  "description": "Multi-agent orchestration system with context-aware task management",
  "configuration": {
    "database": {
      "host": "localhost",
      "port": 5432,
      "name": "orchestrator_db",
      "credentials": {
        "username": "admin",
        "password": "secret"
      }
    },
    "server": {
      "host": "0.0.0.0",
      "port": 9090,
      "ssl": {
        "enabled": true,
        "certPath": "/etc/ssl/cert.pem",
        "keyPath": "/etc/ssl/key.pem"
      }
    }
  },
  "features": {
    "contextSearch": true,
    "consensus": true,
    "embedding": true
  },
  "agents": [
    {
      "id": "claude-code",
      "type": "CLAUDE",
      "enabled": true
    },
    {
      "id": "codex-cli",
      "type": "CODEX",
      "enabled": true
    }
  ],
  "dependencies": {
    "kotlin": "2.2.10",
    "ktor": "3.3.0",
    "duckdb": "1.4.0"
  }
}
